<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- seekbar的滑块样式 -->
    <!-- 按下状态 -->
    <item android:drawable="@drawable/gas_thumb_small" android:state_pressed="true" />
    <!-- 普通无焦点状态 -->
    <item android:drawable="@drawable/gas_thumb_small" android:state_focused="false" android:state_pressed="false"/>

    <item>
        <shape>
            <size
                android:height="5dp"
                android:width="5dp"/>
        </shape>
    </item>
</selector>